<configuration>
    <system.webServer>
        <handlers>
            <add name="iisnode" path="bundle/main.js" verb="*" modules="iisnode_x86"/>
        </handlers>
        <rewrite>
            <rules>
                <rule name="LogFile" patternSyntax="ECMAScript" stopProcessing="true">
                    <match url="^[a-zA-Z0-9_\-]+\.js\.logs\/\d+\.txt$"/>
                </rule>
                <rule name="StaticContent">
                    <action type="Rewrite" url="bundle/programs/web.browser{REQUEST_URI}"/>
                </rule>
                <rule name="DynamicContent">
                    <conditions>
                        <add input="{REQUEST_FILENAME}" matchType="IsFile" negate="True"/>
                    </conditions>
                    <action type="Rewrite" url="bundle/main.js"/>
                </rule>
            </rules>
        </rewrite>
        <iisnode configOverrides="../iisnode.yml"/>
    </system.webServer>
    <appSettings>
        <add key="MONGO_URL" value="mongodb://localhost:27017,localhost:27018,localhost:27019/jobs?replicaSet=rs1&readPreference=primaryPreferred&w=majority" />
        <add key="MONGO_OPLOG_URL" value="mongodb://localhost:27017/local />
		<add key="METEOR_SETTINGS" value="{&quot;jobCollections&quot;: [{&quot;name&quot;: &quot;student-transcript-in&quot;,&quot;adminUserIds&quot;: [],&quot;adminGroups&quot;: [&quot;Enterprise Systems&quot;],&quot;scheduledJobs&quot;: [&quot;getTranscriptIdsFromOCAS&quot;]},{&quot;name&quot;: &quot;student-transcript-out&quot;,&quot;adminUserIds&quot;: [],&quot;adminGroups&quot;: [&quot;Enterprise Systems&quot;],&quot;scheduledJobs&quot;: [&quot;getTranscriptRequestsFromOCAS&quot;]}],&quot;public&quot;: {&quot;jobCollections&quot;: [{&quot;name&quot;: &quot;student-transcript-in&quot;},{&quot;name&quot;: &quot;student-transcript-out&quot;}],&quot;transcriptToHtmlURL&quot;: &quot;http://transformdoc.georgiantest.com&quot;},&quot;ldap&quot;: {&quot;debug&quot;: true,&quot;domain&quot;: &quot;georgiancollege.ca&quot;,&quot;baseDn&quot;: &quot;ou=Georgian,dc=admin,dc=georgianc,dc=on,dc=ca&quot;,&quot;url&quot;: &quot;ldap://ReplaceMeLdapHost/&quot;,&quot;bindCn&quot;: &quot;cn=ReplaceMeUser,ou=Service Accounts,ou=Georgian,dc=admin,dc=georgianc,dc=on,dc=ca&quot;,&quot;bindPassword&quot;: &quot;ReplaceMePassword&quot;,&quot;autopublishFields&quot;: [&quot;displayName&quot;,&quot;givenName&quot;,&quot;department&quot;,&quot;employeeNumber&quot;,&quot;mail&quot;,&quot;title&quot;,&quot;address&quot;,&quot;phone&quot;],&quot;groupMembership&quot;: [&quot;EnterpriseSystems&quot;,&quot;RegManagers&quot;]}}" />
        <add key="ROOT_URL" value="http://admin.georgiantest.com/transcript"/>
    </appSettings>
</configuration>